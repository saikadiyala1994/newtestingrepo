#!/usr/bin/env groovy
properties([[$class: 'GithubProjectProperty', displayName: '', projectUrlStr: 'https://github.com/saikadiyala1994/newtestingrepo.git/'], pipelineTriggers([githubPush()])])

pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                echo 'make.....' 
            }
        }
        stage('Test'){
            steps {
                echo 'make check.......'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'make publish'
            }
        }
    }
}
